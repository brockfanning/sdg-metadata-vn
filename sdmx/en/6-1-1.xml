          <com:Text xml:lang="en"><![CDATA[ <p> Construction Kit </p>]]></com:Text>
        </gen:ReportedAttribute>
        <gen:ReportedAttribute id="COMPILING_ORG">
          <com:Text xml:lang="en"><![CDATA[ <p> Construction Kit </p>]]></com:Text>
        </gen:ReportedAttribute>
        <gen:ReportedAttribute id="RATIONALE">
          <com:Text xml:lang="en"><![CDATA[ <p> The indicator reflects the level of access to clean water of the urban population as well as the results of providing clean water for the urban population; evaluate the effectiveness of national clean water supply programs; reflects the standard of living of people in urban areas. </p>]]></com:Text>
        </gen:ReportedAttribute>
