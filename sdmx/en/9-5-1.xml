        <gen:ReportedAttribute id="SDG_GOAL">
          <com:Text xml:lang="en"><![CDATA[ <p> Goal 9: Build resilient infrastructure, promote inclusive and sustainable industrialization, enhance innovation </p>]]></com:Text>
        </gen:ReportedAttribute>
        <gen:ReportedAttribute id="SDG_TARGET">
          <com:Text xml:lang="en"><![CDATA[ <p> Target 9.5: By 2030, ensure the development of information technology infrastructure to each location, especially mountainous, remote, border and island areas; ensure 100% of households have access to information and communication systems (Global target 9.c) </p>]]></com:Text>
        </gen:ReportedAttribute>
        <gen:ReportedAttribute id="SDG_INDICATOR">
          <com:Text xml:lang="en"><![CDATA[ <p> Indicator 9.5.1. Percentage of population with mobile coverage </p>]]></com:Text>
        </gen:ReportedAttribute>
        <gen:ReportedAttribute id="META_LAST_UPDATE">
          <com:Text xml:lang="en"><![CDATA[ <p> June 2021 </p>]]></com:Text>
        </gen:ReportedAttribute>
        <gen:ReportedAttribute id="CONTACT_ORGANISATION">
          <com:Text xml:lang="en"><![CDATA[ <p> Ministry of Information and Communications </p>]]></com:Text>
        </gen:ReportedAttribute>
        <gen:ReportedAttribute id="CONTACT_MAIL">
          <com:Text xml:lang="en"><![CDATA[ <p> 18 Nguyen Du, Hanoi </p>]]></com:Text>
        </gen:ReportedAttribute>
        <gen:ReportedAttribute id="CONTACT_EMAIL">
          <com:Text xml:lang="en"><![CDATA[ <p> banbientap@mic.gov.vn </p>]]></com:Text>
        </gen:ReportedAttribute>
        <gen:ReportedAttribute id="STAT_CONC_DEF">
          <com:Text xml:lang="en"><![CDATA[ <p> The percentage of population with mobile coverage is the percentage of the population served in a mobile coverage area relative to the population of the country. </p>]]></com:Text>
        </gen:ReportedAttribute>
        <gen:ReportedAttribute id="CLASS_SYSTEM">
          <com:Text xml:lang="en"><![CDATA[ <p> The list of administrative units in Vietnam issued together with the Prime Minister&#39;s Decision No. 124/2004/QD-TTg dated July 8, 2004 and the changes updated by the General Statistics Office to date December 31, 2019) </p>]]></com:Text>
        </gen:ReportedAttribute>
