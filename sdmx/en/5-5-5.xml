        <gen:ReportedAttribute id="SDG_GOAL">
          <com:Text xml:lang="en"><![CDATA[ <p> Goal 5: Achieve gender equality, empower and create opportunities for women and girls </p>]]></com:Text>
        </gen:ReportedAttribute>
        <gen:ReportedAttribute id="SDG_TARGET">
          <com:Text xml:lang="en"><![CDATA[ <p> Target 5.5: Ensure full, effective participation and equal opportunity for women&#39;s leadership participation at all levels of policy-making in political, economic and social life (goal) 5.5 global) </p>]]></com:Text>
        </gen:ReportedAttribute>
        <gen:ReportedAttribute id="SDG_INDICATOR">
          <com:Text xml:lang="en"><![CDATA[ <p> Indicator 5.5.5. Ratio of female directors/owners of businesses and cooperatives </p>]]></com:Text>
        </gen:ReportedAttribute>
