          <com:Text xml:lang="en"><![CDATA[ <p> banbientap@gso.gov.vn </p>]]></com:Text>
        </gen:ReportedAttribute>
        <gen:ReportedAttribute id="STAT_CONC_DEF">
          <com:Text xml:lang="en"><![CDATA[ <p> Household income growth rate is the annual growth rate of real household income per capita. </p>]]></com:Text>
        </gen:ReportedAttribute>
