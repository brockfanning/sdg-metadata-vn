        </gen:ReportedAttribute>
        <gen:ReportedAttribute id="CONTACT_MAIL">
          <com:Text xml:lang="en"><![CDATA[ <p> 54 Nguyen Chi Thanh, Dong Da, Hanoi </p>]]></com:Text>
        </gen:ReportedAttribute>
