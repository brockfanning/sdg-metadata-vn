        <gen:ReportedAttribute id="SDG_GOAL">
          <com:Text xml:lang="en"><![CDATA[ <p> Goal 10: Reduce inequality in society </p>]]></com:Text>
        </gen:ReportedAttribute>
        <gen:ReportedAttribute id="SDG_TARGET">
          <com:Text xml:lang="en"><![CDATA[ <p> Target 10.1: By 2030, gradually achieve and maintain the income growth rate of the bottom 40% of the population higher than the national average growth rate (Global 10.1 target) </p>]]></com:Text>
        </gen:ReportedAttribute>
        <gen:ReportedAttribute id="SDG_INDICATOR">
          <com:Text xml:lang="en"><![CDATA[ <p> Indicator 10.1.2. Growth rate of household expenditure per capita </p>]]></com:Text>
        </gen:ReportedAttribute>
        <gen:ReportedAttribute id="META_LAST_UPDATE">
          <com:Text xml:lang="en"><![CDATA[ <p> June 2021 </p>]]></com:Text>
        </gen:ReportedAttribute>
