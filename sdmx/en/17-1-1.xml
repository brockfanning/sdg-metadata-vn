        <gen:ReportedAttribute id="SDG_GOAL">
          <com:Text xml:lang="en"><![CDATA[ <p> Goal 17: Strengthen implementation and promote global partnerships for sustainable development </p>]]></com:Text>
        </gen:ReportedAttribute>
        <gen:ReportedAttribute id="SDG_TARGET">
          <com:Text xml:lang="en"><![CDATA[ <p> Goal 17.1: Promote a multilateral, open, non-discriminatory, fair and international law-based trading system within the framework of the World Trade Organization, including promoting an end to it Negotiating Doha Development Agenda (global 17.10 target) </p>]]></com:Text>
        </gen:ReportedAttribute>
        <gen:ReportedAttribute id="SDG_INDICATOR">
          <com:Text xml:lang="en"><![CDATA[ <p> Indicator 17.1.1. Average import tax rate </p>]]></com:Text>
        </gen:ReportedAttribute>
        <gen:ReportedAttribute id="META_LAST_UPDATE">
          <com:Text xml:lang="en"><![CDATA[ <p> June 2021 </p>]]></com:Text>
        </gen:ReportedAttribute>
