        <gen:ReportedAttribute id="SDG_GOAL">
          <com:Text xml:lang="en"><![CDATA[ <p> Goal 4: Ensure quality, equitable, inclusive education and promote lifelong learning opportunities for all </p>]]></com:Text>
        </gen:ReportedAttribute>
        <gen:ReportedAttribute id="SDG_TARGET">
          <com:Text xml:lang="en"><![CDATA[ <p> Target 4.2: By 2030, ensure that all girls and boys have access to development, early childhood care and quality early childhood education to be ready for primary school (Section) pepper 4.2 worldwide) </p>]]></com:Text>
        </gen:ReportedAttribute>
        <gen:ReportedAttribute id="SDG_INDICATOR">
          <com:Text xml:lang="en"><![CDATA[ <p> Indicator 4.2.2. Rate of mobilization of 5-year-old children to attend kindergarten </p>]]></com:Text>
        </gen:ReportedAttribute>
        <gen:ReportedAttribute id="META_LAST_UPDATE">
          <com:Text xml:lang="en"><![CDATA[ <p> June 2021 </p>]]></com:Text>
        </gen:ReportedAttribute>
        <gen:ReportedAttribute id="CONTACT_ORGANISATION">
          <com:Text xml:lang="en"><![CDATA[ <p> Ministry of Education and Training </p>]]></com:Text>
        </gen:ReportedAttribute>
        <gen:ReportedAttribute id="CONTACT_MAIL">
          <com:Text xml:lang="en"><![CDATA[ <p> No. 35 Dai Co Viet, Hanoi </p>]]></com:Text>
        </gen:ReportedAttribute>
        <gen:ReportedAttribute id="CONTACT_EMAIL">
          <com:Text xml:lang="en"><![CDATA[ <p> bogddt@moet.gov.vn </p>]]></com:Text>
        </gen:ReportedAttribute>
        <gen:ReportedAttribute id="STAT_CONC_DEF">
          <com:Text xml:lang="en"><![CDATA[ <p> The rate of enrollment of 5-year-old children in kindergarten is the percentage of 5-year-old children attending kindergarten, preschool and independent kindergarten compared to the total population at the age of 5 years. </p>]]></com:Text>
        </gen:ReportedAttribute>
        <gen:ReportedAttribute id="CLASS_SYSTEM">
          <com:Text xml:lang="en"><![CDATA[ <p> The list of administrative units in Vietnam issued together with the Prime Minister&#39;s Decision No. 124/2004/QD-TTg dated July 8, 2004 and the changes updated by the General Statistics Office to date December 31, 2019) </p>]]></com:Text>
        </gen:ReportedAttribute>
        <gen:ReportedAttribute id="SOURCE_TYPE">
          <com:Text xml:lang="en"><![CDATA[ <p> - Number of 5-year-old children attending kindergarten and preschool in school year t: Statistical reporting regime promulgated by the Ministry of Education and Training; </p>
 <p> - Total number of 5-year-old children: Statistical survey. </p>]]></com:Text>
        </gen:ReportedAttribute>
        <gen:ReportedAttribute id="COLL_METHOD">
          <com:Text xml:lang="en"><![CDATA[ <p> - Number of 5-year-old children attending kindergarten and preschool in school year t: Departments of Education and Training of provinces/cities under central authority collect information from the form: 01-MN-DN </p>
 <p> - Total number of 5-year-old children: Collected through the GSO census </p>
 <p> - The Ministry of Education and Training summarizes the number of 5-year-old children attending kindergarten and preschool in school year t and the number of children aged 5 from the General Statistics Office to calculate this indicator. </p>]]></com:Text>
        </gen:ReportedAttribute>
        <gen:ReportedAttribute id="REL_CAL_POLICY">
          <com:Text xml:lang="en"><![CDATA[ <p> Year </p>]]></com:Text>
        </gen:ReportedAttribute>
