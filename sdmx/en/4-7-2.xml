        <gen:ReportedAttribute id="SDG_GOAL">
          <com:Text xml:lang="en"><![CDATA[ <p> Goal 4: Ensure quality, equitable, inclusive education and promote lifelong learning opportunities for all </p>]]></com:Text>
        </gen:ReportedAttribute>
        <gen:ReportedAttribute id="SDG_TARGET">
          <com:Text xml:lang="en"><![CDATA[ <p> Target 4.7: By 2030, ensure that all learners are equipped with the knowledge and skills needed to promote sustainable development (Global Goal 4.7) </p>]]></com:Text>
        </gen:ReportedAttribute>
        <gen:ReportedAttribute id="SDG_INDICATOR">
          <com:Text xml:lang="en"><![CDATA[ <p> Indicator 4.7.2. Percentage of schools with basic education programs on sex, prevention of violence and abuse; provide knowledge about HIV </p>]]></com:Text>
        </gen:ReportedAttribute>
        <gen:ReportedAttribute id="META_LAST_UPDATE">
