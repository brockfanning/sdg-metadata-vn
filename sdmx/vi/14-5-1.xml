        <gen:ReportedAttribute id="SDG_GOAL">
          <com:Text xml:lang="vi"><![CDATA[<p>Mục tiêu 14: Bảo tồn và sử dụng bền vững đại dương, biển và nguồn lợi biển đểphát triển bền vững</p>]]></com:Text>
        </gen:ReportedAttribute>
        <gen:ReportedAttribute id="SDG_TARGET">
          <com:Text xml:lang="vi"><![CDATA[<p>Mục tiêu 14.5: Đến năm 2030, diện tích các khu bảo tồn biển và ven biển đạt 3-5% diện tích tự nhiên, dựa trên cơ sở khoa học và phù hợp với luật pháp quốc gia và quốc tế (mục tiêu 14.5 toàn cầu)</p>]]></com:Text>
        </gen:ReportedAttribute>
        <gen:ReportedAttribute id="SDG_INDICATOR">
          <com:Text xml:lang="vi"><![CDATA[<p>Chỉ tiêu 14.5.1. Diện tích các khu vực bảo tồn biển</p>]]></com:Text>
        </gen:ReportedAttribute>
        <gen:ReportedAttribute id="META_LAST_UPDATE">
          <com:Text xml:lang="vi"><![CDATA[<p>tháng 6/2021</p>]]></com:Text>
        </gen:ReportedAttribute>
        <gen:ReportedAttribute id="CONTACT_ORGANISATION">
          <com:Text xml:lang="vi"><![CDATA[<p>Bộ Nông nghiệp và Phát triển nông thôn</p>]]></com:Text>
        </gen:ReportedAttribute>
        <gen:ReportedAttribute id="CONTACT_MAIL">
          <com:Text xml:lang="vi"><![CDATA[<p>Số 2, Ngọc Hà, Ba Đình, Hà Nội</p>]]></com:Text>
